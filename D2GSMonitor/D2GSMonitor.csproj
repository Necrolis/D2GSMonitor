<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net9.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <ApplicationManifest>app.manifest</ApplicationManifest>
    <Copyright>2025</Copyright>
    <PackageProjectUrl>https://github.com/Necrolis/D2GSMonitor</PackageProjectUrl>
    <RepositoryUrl>https://github.com/Necrolis/D2GSMonitor</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <Authors>Necrolis</Authors>
    <Title>$(AssemblyName)</Title>
    <PackageId />
    <StartupObject>D2GSMonitor.Program</StartupObject>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Win32.Registry" Version="5.0.0" />
    <PackageReference Include="Newtonsoft.Json" Version="13.0.2" />
    <PackageReference Include="Telnet" Version="0.11.3" />
  </ItemGroup>

</Project>
